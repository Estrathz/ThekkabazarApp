// iOS Build Configuration for Thekkabazar
// This file contains build settings for optimized iOS builds

// Version and Build Number
MARKETING_VERSION = 22.0
CURRENT_PROJECT_VERSION = 22

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.thekkabazar

// Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 13.0

// Optimization Settings
GCC_OPTIMIZATION_LEVEL = s
SWIFT_OPTIMIZATION_LEVEL = -O
SWIFT_COMPILATION_MODE = wholemodule

// Disable Bitcode
ENABLE_BITCODE = NO

// Enable App Transport Security
ENABLE_APP_TRANSPORT_SECURITY = YES

// Code Signing
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = 

// Build Settings for Release
GCC_GENERATE_DEBUGGING_SYMBOLS = NO
STRIP_INSTALLED_PRODUCT = YES
COPY_PHASE_STRIP = YES

// Enable Dead Code Stripping
DEAD_CODE_STRIPPING = YES

// Enable Link Time Optimization
LLVM_LTO = YES

// Swift Compiler Flags
SWIFT_ACTIVE_COMPILATION_CONDITIONS = RELEASE 